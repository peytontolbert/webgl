{
  "name": "gta5-terrain-viewer",
  "version": "1.0.0",
  "description": "WebGL viewer for GTA5 terrain",
  "type": "module",
  "scripts": {
    "dev": "vite --host 127.0.0.1",
    "build": "vite build && npm run sync:assets",
